@font-face {
  font-family: "Inter";
  src: url("/fonts/Inter-Regular.eot");
  src: url("/fonts/Inter-Regular.eot?#iefix") format("embedded-opentype"),
    url("/fonts/Inter-Regular.woff") format("woff"),
    url("/fonts/Inter-Regular.ttf") format("truetype");
  font-style: normal;
  font-weight: 400;
}

